                                <TextBlock x:Name="LabConnectUserType" IsHitTestVisible="False" Text="UserType" VerticalAlignment="Center" Foreground="{StaticResource ColorBrushGray1}" Margin="0,0,12,0" />
                            </StackPanel>
                        </StackPanel>
                    </local:MyCard>
                    <!--操作栏-->
                    <local:MyCard x:Name="PanOperation" Title="大厅操作" Margin="10" Grid.Row="3">
                        <StackPanel Orientation="Vertical" Margin="10,35,10,10">
