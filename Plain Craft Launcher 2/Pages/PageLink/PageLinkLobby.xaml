                                <TextBlock x:Name="LabFinishPing" IsHitTestVisible="False" Text="-ms" VerticalAlignment="Center" Foreground="{StaticResource ColorBrushGray1}" Margin="0,0,12,0" />
                            </StackPanel>
                            <StackPanel x:Name="BtnFinishId" Margin="5" Orientation="Horizontal" Background="{StaticResource ColorBrushSemiTransparent}"
                                        ToolTip="大厅编号" ToolTipService.Placement="MousePoint" ToolTipService.InitialShowDelay="100" ToolTipService.VerticalOffset="-35">
