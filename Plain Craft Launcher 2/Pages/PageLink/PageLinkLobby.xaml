                                <TextBlock x:Name="LabConnectUserName" IsHitTestVisible="False" Text="UserName" VerticalAlignment="Center" Foreground="{StaticResource ColorBrushGray1}" Margin="0,0,12,0" />
                            </StackPanel>
                            <TextBlock x:Name="SplitLineBeforeUserType" Text="|" Opacity="0.3" Margin="0,8.8,0,8" RenderTransformOrigin="0.5,0.5" Visibility="{Binding ElementName=LabConnectUserType,Path=Visibility}">
